{
  "name": "game",
  "version": "0.1.3",
  "private": true,
  "author": "dream-canteen",
  "description": "dream-canteen",
  "main": "./public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@pixi/react": "^7.1.1",
    "fbjs": "^3.0.5",
    "pixi.js": "^7.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "prettier": "prettier --config .prettierrc.js src/**/*.{ts,tsx}",
    "format": "yarn prettier --write",
    "lint": "eslint src/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit",
    "unused": "ts-node --compilerOptions='{\"target\":\"es5\",\"module\":\"commonjs\"}' scripts/unused-exports.ts",
    "start": "PORT=8080 react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "build::windows": "yarn build && electron-builder --windows --config.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "rules": {
      "import/order": "warn",
      "sort-keys": "warn"
    },
    "plugins": [
      "import"
    ],
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 electron version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/fbjs": "^3.0.10",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.1.0",
    "react-scripts": "5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.4.2"
  },
  "build": {
    "appId": "com.electron.dreamcanteen",
    "productName": "Dream Canteen",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "target": "portable"
    }
  }
}